
public class Main
{
     public static void main(String[] arg)
     {
     int javaScore=50;
    		 if (javaScore >90)
    		 {
    			 System .out.println("成绩优秀");
    		 }
    		 if (javaScore >60){
    			 System.out.println("成绩合格");
    		 }else{
    			 System .out.println("成绩不合格");
    		 }
    		 if (javaScore <60) 
    			 System.out.println("重修");
	 }
}
