package com.lanou3g.extend;

public class Car {
	private int wheels;
	private String color;
	public int getWheels() {
		return wheels;
	}
	public void setWheels(int wheels) {
		this.wheels = wheels;
	}
	public String getColor() {
		System.out.println("------我是父类中setCar方法"+color);
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	

}
