
public class Main2 {
    public static void main (String[] args){
    	System.out.println("1");
    	int w=51 ;
    	while (w<50) {
    		System.out.println(w);
    		//w++;
    		w*=7;
    		//break;
    	}//System.out.println(w);
    	int progress=1;
    	/*do-while与while的区别
    	 * do-while是先执行一次循环体，然后在判断
    	 * while是先判断然后在执行循环体
    	 */
    	
    	do {
			progress+=2;
			System.out.print(progress);
			 System.out.print("\n");
		} while (progress<10);
    	int i=100;
    	do {
			System .out.println("好好学习天天向上");
			i--;
		} while (i==0);
    	
    	
    	
    }
}
